# Función para calcular el área de un rectángulo
def calcular_area_rectangulo(1):
    largo = float(input("Ingresa el largo del rectángulo: "))
    ancho = float(input("Ingresa el ancho del rectángulo: "))
    
    # Calcular el área
    area = largo * ancho
    
    # Mostrar el resultado
    print(f"El área del rectángulo es: {area} unidades cuadradas.")

# Llamar a la función
calcular_area_rectangulo(1)
